@model AnlabMvc.Models.CyberSource.ReceiptResponseModel
@{
    ViewData["Title"] = "Receipt";
    //var paymentDict = (Dictionary<string, string>)ViewBag.PaymentDictionary;
}

@if (ViewBag.Declined != null && ViewBag.Declined == true)
{
    <h1>Your Payment has been declined or other errors detected.</h1>
}
else
{
    <h1>Thank you for your payment of <i class="fa fa-usd">@Model.Auth_Amount</i></h1>

    <h2>If you still see the Pay button on the results, it make take a minute for the payment to process.</h2>
    <a class="btn btn-default" asp-controller="Results" asp-action="Link" asp-route-id="@ViewBag.ShareId">Back to Results</a>
}
